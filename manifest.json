{
    "manifest_version": 2,

    "name": "One-click",
    "description": "This extension demonstrates a browser action with kittens.",
    "version": "1.1",

    "permissions": [
        "tabs", "http://*/*", "https://*/*"
    ],
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    }
}